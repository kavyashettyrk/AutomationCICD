<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE suite SYSTEM "https://testng.org/testng-1.0.dtd">
<suite parallel="tests" name="Suite">
<groups>
<run>
<include name="ErrorHandling"/>
</run>
</groups>

  <test thread-count="5" name="Submit Order">
    <classes>
    <class name="rahulshettyacademy.Tests.SubmitOrderTest"/>
    </classes>
  </test> <!-- Test -->
      
  <test thread-count="5" name="Error Validations">
    <classes>
      <class name="rahulshettyacademy.Tests.ErrorValidation"/>
    </classes>
  </test>
  
  <test thread-count="5" name="Base Test">
    <classes>
    <class name="rahulshettyacademy.TestComponents.BaseTest"/>
    </classes>
  </test>
  
</suite> <!-- Suite -->


  